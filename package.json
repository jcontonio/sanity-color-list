{
  "name": "sanity-plugin-sanity-color-list",
  "version": "1.0.0",
  "description": "Displays a list of colors",
  "keywords": [
    "sanity",
    "sanity-plugin"
  ],
  "homepage": "https://github.com/jcontonio/sanity-color-list#readme",
  "bugs": {
    "url": "https://github.com/jcontonio/sanity-color-list/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/jcontonio/sanity-color-list.git"
  },
  "license": "MIT",
  "author": "Jay Contonio <jay@conton.io>",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "source": "./src/index.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.esm.js",
      "default": "./dist/index.esm.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.esm.js",
  "source": "./src/index.ts",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "sanity.json",
    "src",
    "v2-incompatible.js"
  ],
  "scripts": {
    "build": "run-s clean && plugin-kit verify-package --silent && pkg-utils build --strict && pkg-utils --strict",
    "clean": "rimraf dist",
    "format": "prettier --write --cache --ignore-unknown .",
    "link-watch": "plugin-kit link-watch",
    "lint": "eslint .",
    "prepublishOnly": "run-s build",
    "watch": "pkg-utils watch --strict"
  },
  "dependencies": {
    "@ctrl/tinycolor": "^3.4.1",
    "@sanity/incompatible-plugin": "^1.0.4",
    "@sanity/ui": "^1.8.2",
    "@types/styled-components": "^5.1.28"
  },
  "devDependencies": {
    "@sanity/pkg-utils": "^2.4.9",
    "@sanity/plugin-kit": "^3.1.10",
    "@types/react": "^18.2.24",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-sanity": "^6.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.3",
    "prettier-plugin-packagejson": "^2.4.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-is": "^18.2.0",
    "rimraf": "^5.0.5",
    "sanity": "^3.17.0",
    "styled-components": "^5.3.11",
    "typescript": "^5.2.2"
  },
  "peerDependencies": {
    "react": "^18",
    "sanity": "^3"
  },
  "engines": {
    "node": ">=14"
  }
}
